{
    "name": "edgar-orozco/versiones",
    "description": "Paquete para administrar ramas, versiones y tags en un proyecto git montado en un servidor web",
    "license": "MIT",
    "authors": [
        {
            "name": "Edgar Orozco",
            "email": "9440039+edgar-orozco@users.noreply.github.com",
            "homepage": "https://lxl.mx"
        }
    ],
    "homepage": "https://github.com/edgar-orozco/versiones",
    "keywords": ["Laravel", "versiones"],
    "require": {
      "php": ">=7.2.0",
      "laravel/framework": ">=7.0.0"
    },
    "require-dev": {
      "phpunit/phpunit": ">=8.5.0"
    },
    "autoload": {
        "psr-4": {
            "EdgarOrozco\\Versiones\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EdgarOrozco\\Versiones\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EdgarOrozco\\Versiones\\VersionesServiceProvider"
            ],
            "aliases": {
                "versiones": "EdgarOrozco\\Versiones\\Facades\\Versiones"
            }
        }
    }
}
